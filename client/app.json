{
  "expo": {
    "userInterfaceStyle": "dark",
    "name": "Insta Stores",
    "slug": "insta-stores",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.shalini_n.clientserver",
      "versionCode": 3,
      "userInterfaceStyle": "dark",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },

    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
